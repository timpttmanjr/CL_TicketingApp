                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ShippingOrder.OrderNumber, CONVERT(varchar(50), ShippingOrder.OrderDate, 101) AS OrderDate,
             (SELECT CompanyName
            FROM  Company
            WHERE (Id = ShippingOrder.CustomerCompanyID)) AS CustomerCompanyName,
             (SELECT CompanyName
            FROM  Company AS Company_5
            WHERE (Id = ShippingOrder.CarrierCompanyID)) AS CarrierCompanyName, ShippingOrder.a1ActivityType,
             (SELECT CompanyName + ' (' + ISNULL(sCity, 'Unknown City') + ', ' + ISNULL(sState, 'Unknown State') + ')' AS CompanyName
            FROM  Company AS Company_4
            WHERE (Id = ShippingOrder.a1LocationCompanyID)) AS Location1CompanyName, ShippingOrder.a2ActivityType,
             (SELECT CompanyName + ' (' + ISNULL(sCity, 'Unknown City') + ', ' + ISNULL(sState, 'Unknown State') + ')' AS CompanyName
            FROM  Company AS Company_3
            WHERE (Id = ShippingOrder.a2LocationCompanyID)) AS Location2CompanyName, ShippingOrder.a3ActivityType,
             (SELECT CompanyName + ' (' + ISNULL(sCity, 'Unknown City') + ', ' + ISNULL(sState, 'Unknown State') + ')' AS CompanyName
            FROM  Company AS Company_2
            WHERE (Id = ShippingOrder.a3LocationCompanyID)) AS Location3CompanyName, ShippingOrder.a4ActivityType,
             (SELECT CompanyName + ' (' + ISNULL(sCity, 'Unknown City') + ', ' + ISNULL(sState, 'Unknown State') + ')' AS CompanyName
            FROM  Company AS Company_1
            WHERE (Id = ShippingOrder.a4LocationCompanyID)) AS Location4CompanyName, ISNULL(ShippingOrder.CustomerBillAmount, 0) + ISNULL(ShippingOrder.ExtraCharge1Amount, 0) AS CustomerBillAmount, ISNULL(ShippingOrder.CarrierPayAmount, 0) + ISNULL(ShippingOrder.ExtraPay1Amount, 0) AS CarrierPayAmount, ISNULL(ShippingOrder.CustomerBillAmount, 0) 
         - ISNULL(ShippingOrder.CarrierPayAmount, 0) + ISNULL(ShippingOrder.ExtraCharge1Amount, 0) - ISNULL(ShippingOrder.ExtraPay1Amount, 0) AS NetProfit, ISNULL(ShippingOrder.a1CommodityType, '') + ISNULL(' ' + ShippingOrder.a2CommodityType, '') + ISNULL(' ' + ShippingOrder.a3CommodityType, '') + ISNULL(' ' + ShippingOrder.a4CommodityType, '') AS Commodity,
		 LoadType, Carrier.Contact AS CarrierContact, shippingOrder.CustomerContact AS CustomerContact
FROM  Company AS Customer INNER JOIN
         ShippingOrder ON Customer.Id = ShippingOrder.CustomerCompanyID LEFT OUTER JOIN
         Company AS Carrier ON ShippingOrder.CarrierCompanyID = Carrier.Id
		
WHERE (ShippingOrder.OrderDate BETWEEN @StartDate AND @EndDate)
ORDER BY ShippingOrderID</CommandText>
